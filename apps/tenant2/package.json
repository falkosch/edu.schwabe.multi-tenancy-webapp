{
  "name": "@edu.schwabe.multi-tenancy-webapp/angularjs-tenant2",
  "description": "Second example tenant for the AngularJS multi-tenant web app prototype",
  "version": "0.0.1",
  "author": {
    "name": "Falko Schwabe",
    "url": "https://bitbucket.org/falko_schwabe/edu.schwabe.multi-tenancy-webapp"
  },
  "private": true,
  "@edu.schwabe.multi-tenancy-webapp/angularjs-tenant2": {
    "baseURL": "/",
    "entryModule": "tenant2",
    "shortName": "ngTenant2",
    "language": {
      "allAvailable": [
        "de",
        "en"
      ],
      "fallback": "en",
      "default": "en",
      "partialsUrlTemplate": "{part}/i18n/{lang}.json",
      "dynamicLocalesUrlTemplate": "./i18n/angular-locale_{{locale}}.js"
    }
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:ci": "webpack --config webpack.prod.js --env.noBundleAnalyzer && node ../../tools/hash-output.js \"./deploy\"",
    "build:serve": "static-server dist/",
    "clean": "rimraf ./reports ./dist ./deploy ./node_modules/.cache",
    "clean:ci": "npm run clean",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "test": "karma start",
    "test:ci": "karma start karma-ci.conf",
    "test:debug": "karma start --browsers ChromiumDebugging --reporters kjhtml",
    "test:precommit": "karma start karma-precommit.conf"
  },
  "dependencies": {
    "@edu.schwabe.multi-tenancy-webapp/angularjs-base-app": "^0.0.1",
    "@edu.schwabe.multi-tenancy-webapp/base-service-worker": "^0.0.1",
    "@uirouter/angularjs": "^1.0.22",
    "angular": "^1.7.8",
    "angular-animate": "^1.7.8",
    "angular-aria": "^1.7.8",
    "angular-dynamic-locale": "^0.1.37",
    "angular-i18n": "^1.7.8",
    "angular-material": "^1.1.20",
    "angular-messages": "^1.7.8",
    "angular-sanitize": "^1.7.8",
    "angular-translate": "^2.18.1",
    "angular-translate-handler-log": "^2.18.1",
    "angular-translate-interpolation-messageformat": "^2.18.1",
    "angular-translate-loader-partial": "^2.18.1",
    "angular-translate-loader-static-files": "^2.18.1",
    "angular-translate-loader-url": "^2.18.1",
    "core-js": "^3.2.1",
    "default-passive-events": "^1.0.10",
    "lodash": "^4.17.15",
    "material-icons": "^0.3.1",
    "moment": "^2.24.0",
    "regenerator-runtime": "^0.13.3",
    "rx-angular": "^1.1.3",
    "uuid-browser": "^3.1.0"
  },
  "devDependencies": {
    "rimraf": "^2.7.1",
    "static-server": "^2.2.1",
    "webpack": "^4.39.2",
    "webpack-dev-server": "^3.8.0"
  }
}
