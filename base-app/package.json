{
  "name": "@edu.schwabe.webapp-prototypes/angularjs-base-app",
  "description": "Base app of an AngularJS multi-tenant web app prototype, build with webpack and babel and tested with karma and jasmine.",
  "version": "0.0.1",
  "author": {
    "name": "Falko Schwabe",
    "url": "https://bitbucket.org/falko_schwabe/edu.schwabe.webapp-prototypes"
  },
  "private": true,
  "@edu.schwabe.webapp-prototypes/angularjs-base-app": {
    "ngAppModule": "index",
    "shortName": "ngBaseApp",
    "language": {
      "allAvailable": [
        "de",
        "en"
      ],
      "fallback": "en",
      "default": "en",
      "partialsUrlTemplate": "{part}/i18n/{lang}.json"
    }
  },
  "scripts": {
    "clean": "rimraf ./reports ./dist ./deploy",
    "clean:ci": "npm run clean",
    "start": "npm run test",
    "test": "karma start",
    "test:ci": "karma start karma-ci.conf",
    "test:debug": "karma start --browsers ChromeDebugging --reporters kjhtml",
    "test:precommit": "karma start karma-precommit.conf"
  },
  "peerDependencies": {
    "@uirouter/angularjs": "^1.0.22",
    "angular": "^1.7.8",
    "angular-animate": "^1.7.8",
    "angular-aria": "^1.7.8",
    "angular-material": "^1.1.19",
    "angular-messages": "^1.7.8",
    "angular-sanitize": "^1.7.8",
    "angular-translate": "^2.18.1",
    "angular-translate-handler-log": "^2.18.1",
    "angular-translate-interpolation-messageformat": "^2.18.1",
    "angular-translate-loader-partial": "^2.18.1",
    "angular-translate-loader-static-files": "^2.18.1",
    "angular-translate-loader-url": "^2.18.1",
    "lodash": "^4.17.14",
    "material-icons": "^0.3.1",
    "moment": "^2.24.0",
    "rx-angular": "^1.1.3",
    "uuid-browser": "^3.1.0"
  },
  "devDependencies": {
    "@uirouter/angularjs": "*",
    "angular": "*",
    "angular-animate": "*",
    "angular-aria": "*",
    "angular-material": "*",
    "angular-messages": "*",
    "angular-sanitize": "*",
    "angular-translate": "*",
    "angular-translate-handler-log": "*",
    "angular-translate-interpolation-messageformat": "*",
    "angular-translate-loader-partial": "*",
    "angular-translate-loader-static-files": "*",
    "angular-translate-loader-url": "*",
    "lodash": "*",
    "material-icons": "*",
    "moment": "*",
    "rx-angular": "*",
    "uuid-browser": "*"
  }
}
