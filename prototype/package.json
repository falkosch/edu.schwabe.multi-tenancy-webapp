{
  "name": "@edu.schwabe.webapp-prototypes/angularjs-prototype",
  "description": "An AngularJS web app prototype, build with webpack and babel and tested with karma and jasmine.",
  "version": "0.0.1",
  "author": {
    "name": "Falko Schwabe",
    "url": "https://bitbucket.org/falko_schwabe/edu.schwabe.webapp-prototypes"
  },
  "private": true,
  "@edu.schwabe.webapp-prototypes/angularjs-prototype": {
    "shortName": "ngPrototype",
    "language": {
      "allAvailable": [
        "de",
        "en"
      ],
      "fallback": "en",
      "default": "en",
      "partialsUrlTemplate": "{part}/i18n/{lang}.json"
    }
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:ci": "webpack --config webpack.prod.js --env.noBundleAnalyzer --env.noProgress",
    "build:serve": "static-server dist/",
    "clean": "rimraf ./dist ./deploy ./reports ./docs",
    "clean:ci": "npm run clean",
    "docs": "jsdoc -c jsdoc.conf.js",
    "docs:ci": "npm run docs",
    "docs:serve": "static-server docs/",
    "lint": "eslint . --fix && stylelint . --fix",
    "lint:ci": "npm run lint:ci-js && npm run lint:ci-scss",
    "lint:ci-js": "eslint . -f checkstyle -o reports/eslint.checkstyle.xml",
    "lint:ci-scss": "stylelint . --custom-formatter node_modules/stylelint-checkstyle-formatter > reports/stylelint.checkstyle.xml",
    "lint:precommit": "lint-staged",
    "precommit": "npm run lint:precommit && npm run test:precommit",
    "sonar:ci": "npm run sonar:ci-js && npm run sonar:ci-scss && sonar-scanner",
    "sonar:ci-js": "eslint . -f json -o reports/eslint.json",
    "sonar:ci-scss": "stylelint . -f json > reports/stylelint.json",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "test": "karma start",
    "test:ci": "karma start karma-ci.conf",
    "test:debug": "karma start --browsers ChromeDebugging --reporters kjhtml",
    "test:precommit": "karma start karma-precommit.conf"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "@uirouter/angularjs": "^1.0.22",
    "angular": "^1.7.8",
    "angular-animate": "^1.7.8",
    "angular-aria": "^1.7.8",
    "angular-material": "^1.1.18",
    "angular-messages": "^1.7.8",
    "angular-sanitize": "^1.7.8",
    "angular-translate": "^2.18.1",
    "angular-translate-handler-log": "^2.18.1",
    "angular-translate-interpolation-messageformat": "^2.18.1",
    "angular-translate-loader-partial": "^2.18.1",
    "angular-translate-loader-static-files": "^2.18.1",
    "angular-translate-loader-url": "^2.18.1",
    "default-passive-events": "^1.0.10",
    "jslinq": "^1.0.21",
    "lodash": "^4.17.11",
    "material-icons": "^0.3.1",
    "messageformat": "^2.1.0",
    "moment": "^2.24.0",
    "rx-angular": "^1.1.3",
    "uuid-browser": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "@types/angular": "^1.6.54",
    "@types/angular-animate": "^1.5.10",
    "@types/angular-material": "^1.1.67",
    "@types/angular-mocks": "^1.7.0",
    "@types/angular-translate": "^2.16.0",
    "@types/jasmine": "^3.3.12",
    "@types/lodash": "^4.14.123",
    "angular-mocks": "^1.7.8",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-istanbul": "^5.1.2",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^2.0.1",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-problems": "^2.0.0",
    "eslint-plugin-angular": "^4.0.0",
    "eslint-plugin-compat": "^3.1.1",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-sonarjs": "^0.3.0",
    "file-loader": "^3.0.1",
    "filemanager-webpack-plugin": "^2.0.5",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "ignore-loader": "^0.1.2",
    "image-webpack-loader": "^4.6.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "jasmine-core": "^3.4.0",
    "jsdoc": "^3.5.5",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-summary-reporter": "^1.6.0",
    "karma-webpack": "^4.0.0-rc.6",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.6.0",
    "ngtemplate-loader": "^2.0.1",
    "node-sass": "^4.11.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^7.0.1",
    "postcss-preset-env": "^6.6.0",
    "puppeteer": "^1.14.0",
    "resolve-url-loader": "^3.1.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "serviceworker-webpack-plugin": "^1.0.1",
    "sonarqube-scanner": "^2.4.0",
    "static-server": "^2.2.1",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-checkstyle-formatter": "^0.1.2",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-no-unsupported-browser-features": "^3.0.2",
    "stylelint-scss": "^3.6.0",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.2",
    "webapp-webpack-plugin": "^2.7.0",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "lint-staged": {
    "*.js": "eslint --format=codeframe",
    "*.scss": "stylelint"
  }
}
