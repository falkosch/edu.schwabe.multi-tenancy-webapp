{
  "name": "@edu.schwabe.webapp-prototypes/angularjs-prototype",
  "description": "An AngularJS web app prototype, build with webpack and babel and tested with karma and jasmine.",
  "version": "0.0.1",
  "author": {
    "name": "Falko Schwabe",
    "url": "https://bitbucket.org/falko_schwabe/edu.schwabe.webapp-prototypes"
  },
  "private": true,
  "@edu.schwabe.webapp-prototypes/angularjs-prototype": {
    "shortName": "ngPrototype",
    "language": {
      "allAvailable": [
        "de",
        "en"
      ],
      "fallback": "en",
      "default": "en",
      "partialsUrlTemplate": "{part}/i18n/{lang}.json"
    }
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
