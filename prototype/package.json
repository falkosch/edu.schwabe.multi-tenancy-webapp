    "test": "karma start",
    "test:debug": "karma start --browsers Chrome",
    "docs": "jsdoc -c jsdoc.conf.js",
    "docs:serve": "http-server --cors ./docs",
    "lint:js": "eslint . --fix",
    "lint:scss": "stylelint . --fix"
