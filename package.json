    "puppeteer": "^1.19.0",
    "resolve-url-loader": "^3.1.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.1",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "serviceworker-webpack-plugin": "^1.0.1",
    "sonarqube-scanner": "^2.5.0",
    "static-server": "^2.2.1",
    "style-loader": "^0.23.1",
