{
  "name": "@edu.schwabe.multi-tenancy-webapp/multi-tenancy-webapp",
  "description": "AngularJS multi-tenant web app prototype, build with webpack and babel and tested with karma and jasmine.",
  "private": true,
  "author": {
    "name": "Falko Schwabe",
    "url": "https://github.com/falkosch/edu.schwabe.multi-tenancy-webapp"
  },
  "scripts": {
    "build:ci": "lerna run build:ci",
    "clean": "rimraf ./.scannerwork ./reports ./docs ./node_modules/.cache",
    "clean:ci": "npm run clean && lerna run clean:ci",
    "docs": "typedoc",
    "docs:ci": "npm run docs",
    "docs:serve": "static-server docs/",
    "lint": "npm run lint:eslint && npm run lint:tsc && npm run lint:stylelint",
    "lint:eslint": "eslint \"**/*\" --fix",
    "lint:tsc": "tsc --noEmit",
    "lint:stylelint": "stylelint . --fix",
    "lint:ci": "npm run lint:ci-eslint && npm run lint:ci-tsc && npm run lint:ci-stylelint",
    "lint:ci-eslint": "eslint \"**/*\" -f checkstyle -o reports/eslint.checkstyle.xml",
    "lint:ci-tsc": "tsc --noEmit",
    "lint:ci-stylelint": "stylelint . --custom-formatter node_modules/stylelint-checkstyle-formatter > reports/stylelint.checkstyle.xml",
    "lint:precommit": "lint-staged",
    "postinstall": "lerna bootstrap",
    "sonar:ci": "npm run sonar:ci-eslint && npm run sonar:ci-stylelint && sonar-scanner",
    "sonar:ci-eslint": "eslint \"**/*\" -f json -o reports/eslint.json",
    "sonar:ci-stylelint": "stylelint . -f json > reports/stylelint.json",
    "test:ci": "lerna run test:ci",
    "test:precommit": "lerna run test:precommit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:precommit && npm run test:precommit"
    }
  },
  "devDependencies": {
