{
  "name": "@edu.schwabe.webapp-prototypes/webapp-prototypes",
  "description": "AngularJS multi-tenant web app prototype, build with webpack and babel and tested with karma and jasmine.",
  "private": true,
  "author": {
    "name": "Falko Schwabe",
    "url": "https://bitbucket.org/falko_schwabe/edu.schwabe.webapp-prototypes"
  },
  "scripts": {
    "build:ci": "lerna run build:ci",
    "clean": "rimraf ./.scannerwork ./reports ./docs ./node_modules/.cache",
    "clean:ci": "npm run clean && lerna run clean:ci",
    "docs": "typedoc",
    "docs:ci": "npm run docs",
    "docs:serve": "static-server docs/",
    "lint": "npm run lint:eslint && npm run lint:tsc && npm run lint:stylelint",
    "lint:eslint": "eslint \"**/*\" --fix",
    "lint:tsc": "tsc --noEmit",
    "lint:stylelint": "stylelint . --fix",
    "lint:ci": "npm run lint:ci-eslint && npm run lint:ci-tsc && npm run lint:ci-stylelint",
    "lint:ci-eslint": "eslint \"**/*\" -f checkstyle -o reports/eslint.checkstyle.xml",
    "lint:ci-tsc": "tsc --noEmit",
    "lint:ci-stylelint": "stylelint . --custom-formatter node_modules/stylelint-checkstyle-formatter > reports/stylelint.checkstyle.xml",
    "lint:precommit": "lint-staged",
    "postinstall": "lerna bootstrap",
    "sonar:ci": "npm run sonar:ci-eslint && npm run sonar:ci-stylelint && sonar-scanner",
    "sonar:ci-eslint": "eslint \"**/*\" -f json -o reports/eslint.json",
    "sonar:ci-stylelint": "stylelint . -f json > reports/stylelint.json",
    "test:ci": "lerna run test:ci",
    "test:precommit": "lerna run test:precommit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:precommit && npm run test:precommit"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.5.4",
    "@types/angular": "^1.6.55",
    "@types/angular-animate": "^1.5.10",
    "@types/angular-material": "^1.1.68",
    "@types/angular-mocks": "^1.7.0",
    "@types/angular-sanitize": "^1.7.0",
    "@types/angular-translate": "^2.16.0",
    "@types/jasmine": "^3.3.16",
    "@types/lodash": "^4.14.136",
    "@types/rx-angular": "0.0.30",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "angular-mocks": "^1.7.8",
    "babel-eslint": "^11.0.0-beta.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "core-js": "^3.1.4",
    "css-loader": "^3.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-problems": "^3.0.1",
    "eslint-plugin-angular": "^4.0.1",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-markdown": "^1.0.0",
    "eslint-plugin-sonarjs": "^0.4.0",
    "file-loader": "^4.1.0",
    "filemanager-webpack-plugin": "^2.0.5",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.1",
    "ignore-loader": "^0.1.2",
    "image-webpack-loader": "^5.0.0",
